@model IEnumerable<Yarışma.Models.Period>

<div class="page-content-wrapper">
	<div class="page-content">
		<div class="page-content">
			<a class="btn btn-success" asp-area="Management" asp-controller="Period" asp-action="Create">Ekle</a>
			<div class="card radius-15">
				<div class="card-body">
					<div class="card-title">
						<h4 class="mb-0">Yarışma Takvimi Oluştur</h4>
					</div>
					<hr>
					<div class="table-responsive">
						<table class="table table-bordered">
							<thead>
								<tr>

									<th scope="col">Dönem Adı:</th>
									<th scope="col">Yarışma Baslama Tarihi:</th>
									<th scope="col">Yarışma Bitiş Tarihi:</th>
									<th scope="col">Proje Başlama Tarihi:</th>
									<th scope="col">Proje Bitiş Tarihi :</th>
									<th scope="col">Hakem Degerlendirme Başlangıç Tarihi:</th>
									<th scope="col">Hakem Degerlendirme BitişTarihi:</th>
									<th scope="col">#</th>
								</tr>
							</thead>
							<tbody>
								@foreach (var item in Model)
								{
									<tr>
										<td>@item.PeriodName</td>
										<td>

											<a class="btn btn-primary" asp-area="Management"
											   asp-controller="Period" asp-action="Edit" asp-route-id="@item.Id">Düzenle</a>
											<a class="btn btn-danger" asp-area="Management"
											   asp-controller="Period" asp-action="Delete" asp-route-id="@item.Id">Sil</a>
										</td>
										<td>@item.ProjectStartDate</td>
										<td>

											<a class="btn btn-primary" asp-area="Management"
											   asp-controller="Period" asp-action="Edit" asp-route-id="@item.Id">Düzenle</a>
											<a class="btn btn-danger" asp-area="Management"
											   asp-controller="Period" asp-action="Delete" asp-route-id="@item.Id">Sil</a>
										</td>
										<td>@item.ProjectEndDate</td>
										<td>

											<a class="btn btn-primary" asp-area="Management"
											   asp-controller="Period" asp-action="Edit" asp-route-id="@item.Id">Düzenle</a>
											<a class="btn btn-danger" asp-area="Management"
											   asp-controller="Period" asp-action="Delete" asp-route-id="@item.Id">Sil</a>
										</td>
										<td>@item.ContestanStartDate</td>
										<td>

											<a class="btn btn-primary" asp-area="Management"
											   asp-controller="Period" asp-action="Edit" asp-route-id="@item.Id">Düzenle</a>
											<a class="btn btn-danger" asp-area="Management"
											   asp-controller="Period" asp-action="Delete" asp-route-id="@item.Id">Sil</a>
										</td>
										<td>@item.ContestantEndDate</td>
										<td>

											<a class="btn btn-primary" asp-area="Management"
											   asp-controller="Period" asp-action="Edit" asp-route-id="@item.Id">Düzenle</a>
											<a class="btn btn-danger" asp-area="Management"
											   asp-controller="Period" asp-action="Delete" asp-route-id="@item.Id">Sil</a>
										</td>
										<td>@item.JudgeStartDate</td>
										<td>

											<a class="btn btn-primary" asp-area="Management"
											   asp-controller="Period" asp-action="Edit" asp-route-id="@item.Id">Düzenle</a>
											<a class="btn btn-danger" asp-area="Management"
											   asp-controller="Period" asp-action="Delete" asp-route-id="@item.Id">Sil</a>
										</td>
										<td>@item.JudgeEndDate</td>
										<td>

											<a class="btn btn-primary" asp-area="Management"
											   asp-controller="Period" asp-action="Edit" asp-route-id="@item.Id">Düzenle</a>
											<a class="btn btn-danger" asp-area="Management"
											   asp-controller="Period" asp-action="Delete" asp-route-id="@item.Id">Sil</a>
										</td>
									</tr>
								}
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>

