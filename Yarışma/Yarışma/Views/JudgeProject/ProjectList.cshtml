@model List<dynamic>

<table class="table">
    <thead>
        <tr>
            <th>Proje Adı</th>
            <th>Kategori</th>
            <th>Not</th>
            <th>Yorum</th>
            <th>İşlemler</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var project in Model)
        {
            <tr>
                <td>@project.ProjectName</td>
                <td>@project.ProjectCategoryName</td>
                <td>@project.Score</td>
                <td>@project.Comments</td>
                <td>
                    <a asp-action="ProjectDetails" asp-route-projectId="@project.Id" class="btn btn-info">Detay</a>
                </td>
            </tr>
        }
    </tbody>
</table>
